{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "eofline": false,
    "quotemark": [true, "single"],
    "semicolon": [true, "never"],
    "curly": false,
    "ordered-imports": false,
    "max-classes-per-file": false,
    "object-literal-sort-keys": false,
    "variable-name": false
  },
  "rulesDirectory": []
}
